                                 ;					also input to 'delay_micros'.
                                 ;==============================================================================
                                 delay_ms:
00008c 2f28                      	MOV				R18,			R24
                                 loop_dms:
00008d e034                      	LDI				R19,			4
                                 loop_dms2:
00008e ef8a                      	LDI				R24,			250
00008f dfec                      	RCALL			delay_micros
000090 953a                      	DEC				R19
000091 3030                      	CPI				R19,			0
000092 f7d9                      	BRNE			loop_dms2
000093 952a                      	DEC				R18
000094 3020                      	CPI				R18,			0
000095 f7b9                      	BRNE			loop_dms
000096 9508                      	RET
                                 
