00013d e184                      	LDI				R24,			20				; Wait after power-up,
00013e df4d                      	RCALL			delay_ms						; let Vcc stabilize!
00013f 9a2d                      	SBI				PORTB,			5				; Set LCD reset pin HIGH
000140 e08a                      	LDI				R24,			10				; Wait before...
000141 df4a                      	RCALL			delay_ms						; ...raising...
000142 9a2e                      	SBI				PORTB,			6				; ...CS/SS pin (normal lvl)
000143 982c
000144 e281
000145 d033                      	LCD_WRITE_CMD	0x21							; Function set (H=1)
000146 982c
000147 e183
000148 d030                      	LCD_WRITE_CMD	0x13							; Set bias mode 1:48
000149 982c
00014a ec80
00014b d02d                      	LCD_WRITE_CMD	0xC0							; Set Vop (contrast)
00014c 982c
00014d e084
00014e d02a                      	LCD_WRITE_CMD	0x04							; Set temp coefficient
00014f 982c
000150 e280
000151 d027                      	LCD_WRITE_CMD	0x20							; Function set (H=0)
000152 982c
000153 e08c
