0001a1 3030                      	CPI				R19,			0
0001a2 f7d1                      	BRNE			loop
0001a3 982c
0001a4 e880
0001a5 dfd3                      	LCD_WRITE_CMD	0x80
0001a6 982c
0001a7 e480
0001a8 dfd0                      	LCD_WRITE_CMD	0x40 
