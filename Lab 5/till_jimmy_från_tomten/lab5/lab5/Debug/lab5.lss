000157 9184                      	LPM				R24,			Z				; Copy character to reg
000158 3080                      	CPI				R24,			0				; End of string?
000159 f029                      	BREQ			end_of_str						; ...end subroutine!
00015a 9a2c
00015b d004                      	LCD_WRITE_CHR									; Write char to LCD
00015c 01f1                      	MOVW			ZH:ZL,			R3:R2			; Restore string pointer
00015d 9631                      	ADIW			ZH:ZL,			1				; Increment string pointer
00015e cff7                      	RJMP			iterate_str						; keep on iterating...
                                 end_of_str:
00015f 9508                      	RET
                                 
                                 ;==============================================================================
                                 ; Write a character to the LCD.
                                 ; Uses registers:
                                 ;	R1:R0			Initial pointer to character data in program memory,
                                 ;					this is where the result of R16xR17 is stored.
                                 ;   ZH:ZL			"Moving pointer" to character data
                                 ;	R16				Copy of parameter data (character)
                                 ;	R17				Character width (without padding)
                                 ;	R24				Parameter data/char (input to this subroutine)
                                 ;					also used by 'lcd_write_byte'.
                                 ;==============================================================================
                                 lcd_write_char:	
000160 2f08                      	MOV				R16,			R24				; copy char to temp reg
000161 5200                      	SUBI			R16,			ASCII_OFFSET	; subtract ASCII-OFFSET
000162 e015                      	LDI				R17,			CHR_WIDTH		; set multiplier
000163 9f01                      	MUL				R16,			R17				; (ASCII-OFFSET)xCHR_WIDTH
000164 e0f1                      	LDI				ZH,				HIGH(chars << 1); Init pointer
000165 e5e2                      	LDI				ZL,				LOW(chars << 1)
000166 0de0                      	ADD				ZL,				R0				; Set pointer with result from MUL
000167 1df1                      	ADC				ZH,				R1
000168 9184                      	LPM				R24,			Z				; Write 1st segment of char
000169 d00f                      	RCALL			lcd_write_byte
00016a 9631                      	ADIW			ZH:ZL,			1				; Increment pointer
00016b 9184                      	LPM				R24,			Z				; Write 2nd segment of char
00016c d00c                      	RCALL			lcd_write_byte
00016d 9631                      	ADIW			ZH:ZL,			1				; Increment pointer
00016e 9184                      	LPM				R24,			Z				; Write 3rd segment of char
00016f d009                      	RCALL			lcd_write_byte
000170 9631                      	ADIW			ZH:ZL,			1				; Increment pointer
000171 9184                      	LPM				R24,			Z				; Write 4th segment of char
000172 d006                      	RCALL			lcd_write_byte
000173 9631                      	ADIW			ZH:ZL,			1				; Increment pointer
000174 9184                      	LPM				R24,			Z				; Write 5th segment of char
000175 d003                      	RCALL			lcd_write_byte
000176 e080                      	LDI				R24,			0x00			; Write 6th segment of char
000177 d001                      	RCALL			lcd_write_byte					; (padding)
000178 9508                      	RET
                                 
                                 ;==============================================================================
                                 ; Write a byte of data to the LCD, start with the most significant bit.
                                 ; Uses registers:
                                 ;	R16				Copy of parameter data
                                 ;	R17				Bit mask (for filtering MSB)
                                 ;	R18				Counter (number of bits written to LCD)
                                 ;	R24				Parameter data/byte (input to this subroutine)
                                 ;					also used by SET_IO_BIT and CLR_IO_BIT macros.
                                 ;==============================================================================
                                 lcd_write_byte:
000179 982e                      	CBI				PORTB,			6				; Lower CS/SS pin
00017a 2f08                      	MOV				R16,			R24				; Copy data to temp reg
00017b e020                      	LDI				R18,			0				; Reset counter
                                 mask_bit:
00017c e810                      	LDI				R17,			0x80			; Store bit mask
00017d 2310                      	AND				R17,			R16				; Mask out MSB
00017e 3010                      	CPI				R17,			0				; If the MSB is..
00017f f431                      	BRNE			bit_is_1						; ..not a 0, branch!
000180 9180 0102
000182 7b8f
000183 9380 0102                 	CLR_IO_BIT		PORTH,			BIT6_LOW		; Clear data pin
000185 c005                      	RJMP			write_to_lcd					; Get ready to write data
                                 bit_is_1:
000186 9180 0102
000188 6480
000189 9380 0102                 	SET_IO_BIT		PORTH,			BIT6_HIGH		; Set data pin
                                 write_to_lcd:
00018b 9180 0102
00018d 6280
00018e 9380 0102                 	SET_IO_BIT		PORTH,			BIT5_HIGH		; Shift out data..
000190 dee1                      	RCALL			delay_1_micros					; ..by bit-banging..
000191 9180 0102
000193 7d8f
000194 9380 0102                 	CLR_IO_BIT		PORTH,			BIT5_LOW		; ..a clock pulse!
000196 0f00                      	LSL				R16								; Shift data to the left
000197 9523                      	INC				R18								; Increase counter
000198 3028                      	CPI				R18,			8				; Shifted all bits?
000199 f711                      	BRNE			mask_bit						; Move on to next bit
00019a 9a2e                      	SBI				PORTB,			6				; Raise CS/SS pin
00019b 9508                      	RET												; (normal mode)
                                 
