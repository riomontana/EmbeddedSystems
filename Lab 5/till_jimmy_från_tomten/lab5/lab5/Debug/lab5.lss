                                  * keyboard.inc
                                  *
                                  * This file contains a driver for reading the keypad on the
                                  * "iBridge keypad shield". The driver is adapted for a shield mounted
                                  * on a Arduino Mega board (ATmega2560).
                                  *
                                  * Key mapping:
                                  *					-----------------
                                  *					| 1 | 2 | 3 | A |
                                  *					-----------------
                                  *					| 4 | 5 | 6 | B |
                                  *					-----------------
                                  *					| 7 | 8 | 9 | C |
                                  *					-----------------
                                  *					| * | 0 | # | D |
                                  *					-----------------
                                  *
                                  * Created by Mathias Beckius, 25 June 2015, for the course DA346A at
                                  * Malmo University.
                                  *
                                  * Edited by Linus Forsberg och Jimmy kesson 171207
                                  * Added functionality to read_keyboard_num returning character when keyboard is pressed. 
                                  */
                                 
                                 ;==============================================================================
                                 ; Table of characters, for mapping with keys
                                 ;==============================================================================
                                 keys:
0001aa 3130
0001ab 3332
0001ac 3534
0001ad 1036                      	.DB				"0123456", NO_KEY
                                 
                                 ;==============================================================================
                                 ; Definitions of registers, etc. ("constants")
                                 ;==============================================================================
                                 	.EQU			NO_KEY		=	16				; no key was pressed!
                                 
                                 ;==============================================================================
                                 ; Read keyboard
                                 ; Returns the corresponding character for the pressed key. If no key is
                                 ; pressed, then NO_KEY is returned.
                                 ;
                                 ; Uses registers:
                                 ;	R19			Counter to use as pointer 
                                 ;   R24			Register when returning key
                                 ;
                                 ; Important! A minimum delay of 2 NOP's is necessary after setting port,
                                 ; before pin can be read.
                                 ;==============================================================================
                                 read_keyboard:
0001ae e030                      	LDI				R19,			0			; Reset counter
0001af 98a5                      	CBI				PORTG,			5			; CLEAR BIT COLUMN 1
0001b0 9873                      	CBI				PORTE,			3			; CLEAR BIT COLUMN 2
0001b1 9180 0102
0001b3 7f87
0001b4 9380 0102                 	CLR_IO_BIT		PORTH,			BIT3_LOW	; CLEAR BIT COLUMN 3
0001b6 9180 0102
0001b8 7e8f
0001b9 9380 0102                 	CLR_IO_BIT		PORTH,			BIT4_LOW	; CLEAR BIT COLUMN 4
0001bb e082                      	LDI				R24,			2
0001bc debf                      	RCALL			delay_micros
                                 
0001bd 9aa5                      	SBI				PORTG,			5			; SET BIT COLUMN 1
0001be e082                      	LDI				R24,			2
0001bf debc                      	RCALL			delay_micros
0001c0 997d                      	SBIC			PINF,			5			; ROW 1 (1)
0001c1 c04b                      	RJMP			return_key		
0001c2 9533                      	INC				R19							; 1
0001c3 997c                      	SBIC			PINF,			4			; ROW 2 (4)
0001c4 c048                      	RJMP			return_key
0001c5 9533                      	INC				R19							; 2
0001c6 9964                      	SBIC			PINE,			4			; ROW 3 (7)
0001c7 c045                      	RJMP			return_key
0001c8 9533                      	INC				R19							; 3
0001c9 9965                      	SBIC			PINE,			5			; ROW 4 (*)
0001ca c042                      	RJMP			return_key
0001cb 9533                      	INC				R19							; 4
0001cc 98a5                      	CBI				PORTG,			5			; CLEAR BIT COLUMN 1
                                 
0001cd 9a73                      	SBI				PORTE,			3			; SET BIT COLUMN 2
0001ce e082                      	LDI				R24,			2
0001cf deac                      	RCALL			delay_micros
0001d0 997d                      	SBIC			PINF,			5			; ROW 1 (2)
0001d1 c03b                      	RJMP			return_key		
0001d2 9533                      	INC				R19							; 5
0001d3 997c                      	SBIC			PINF,			4			; ROW 2 (5)
0001d4 c038                      	RJMP			return_key
0001d5 9533                      	INC				R19							; 6
0001d6 9964                      	SBIC			PINE,			4			; ROW 3 (8)
0001d7 c035                      	RJMP			return_key
0001d8 9533                      	INC				R19							; 7
0001d9 9965                      	SBIC			PINE,			5			; ROW 4 (0)
0001da c032                      	RJMP			return_key
0001db 9533                      	INC				R19							; 8
0001dc 9873                      	CBI				PORTE,			3			; CLEAR BIT COLUMN 2
                                 
0001dd 9180 0102
0001df 6088
0001e0 9380 0102                 	SET_IO_BIT		PORTH,			BIT3_HIGH	; SET BIT COLUMN 3
0001e2 e082                      	LDI				R24,			2
0001e3 de98                      	RCALL			delay_micros
0001e4 997d                      	SBIC			PINF,			5			; ROW 1 (3)
0001e5 c027                      	RJMP			return_key		
0001e6 9533                      	INC				R19							; 9
0001e7 997c                      	SBIC			PINF,			4			; ROW 2 (6)
0001e8 c024                      	RJMP			return_key
0001e9 9533                      	INC				R19							; 10
0001ea 9964                      	SBIC			PINE,			4			; ROW 3 (9)
0001eb c021                      	RJMP			return_key
0001ec 9533                      	INC				R19							; 11
0001ed 9965                      	SBIC			PINE,			5			; ROW 4 (#)
0001ee c01e                      	RJMP			return_key
0001ef 9533                      	INC				R19							; 12
0001f0 9180 0102
0001f2 7f87
0001f3 9380 0102                 	CLR_IO_BIT		PORTH,			BIT3_LOW	; CLEAR BIT COLUMN 3
                                 
0001f5 9180 0102
0001f7 6180
0001f8 9380 0102                 	SET_IO_BIT		PORTH,			BIT4_HIGH	; SET BIT COLUMN 4
0001fa e082                      	LDI				R24,			2
0001fb de80                      	RCALL			delay_micros
0001fc 997d                      	SBIC			PINF,			5			; ROW 1 (A)
0001fd c00f                      	RJMP			return_key		
0001fe 9533                      	INC				R19							; 13
0001ff 997c                      	SBIC			PINF,			4			; ROW 2 (B)
000200 c00c                      	RJMP			return_key
000201 9533                      	INC				R19							; 14
000202 9964                      	SBIC			PINE,			4			; ROW 3 (C)
000203 c009                      	RJMP			return_key
000204 9533                      	INC				R19							; 15
000205 9965                      	SBIC			PINE,			5			; ROW 4 (D)
000206 c006                      	RJMP			return_key
000207 9533                      	INC				R19							; 16 (NO_KEY)
000208 9180 0102
00020a 7e8f
00020b 9380 0102                 	CLR_IO_BIT		PORTH,			BIT4_LOW	; CLEAR BIT COLUMN 4
                                 return_key:
00020d 2f83                      	MOV				R24,			R19
00020e 9508                      	RET
                                 
