                                 ; Uses registers:
                                 ;	R24				Input parameter data (X s)
                                 ;==============================================================================
                                 delay_micros:
00007c 0000                      	NOP
00007d 0000                      	NOP
00007e 0000                      	NOP
00007f 0000                      	NOP
000080 0000                      	NOP
000081 0000                      	NOP
000082 0000                      	NOP
000083 0000                      	NOP
000084 0000                      	NOP
000085 0000                      	NOP
000086 0000                      	NOP
000087 0000                      	NOP
000088 958a                      	DEC				R24
